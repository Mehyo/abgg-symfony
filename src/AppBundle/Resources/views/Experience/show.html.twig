{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Experience</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Lookingforteam</th>
                <td>{{ entity.lookingForTeam }}</td>
            </tr>
            <tr>
                <th>Rankactual</th>
                <td>{{ entity.rankActual }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ entity.username }}</td>
            </tr>
            <tr>
                <th>Positionssortedbyfavorites</th>
                <td>{{ entity.positionsSortedByFavorites }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('experience') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('experience_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
